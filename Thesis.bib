@inproceedings{el-mowafyPositioningIntegrityAvailability2018,
  title = {Positioning {{Integrity}}, {{Availability}} and {{Precision}} for {{Journey Planning}} and {{Navigation}} Using {{GNSS Integrated}} with {{Low-Cost Sensors}}},
