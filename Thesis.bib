@inproceedings{kimKrigingBasedWeighting2022,
  title = {Kriging {{Based Weighting Model}} for {{Network RTK Coverage Expansion}}},
  author = {Kim, Bu-Gyeom and Kee, Changdon and Kim, Donguk},
