@thesis{robertwilliamsAerialVisionBased2022,
  type = {Thesis},
  title = {Aerial {{Vision Based Relative Positioning}} and {{Ground Vehicle Speed Estimation}} for {{Cooperative UAV-UGV Automated Platoons}}},
  author = {Robert Williams},
  date = {2022-12},
  institution = {{Auburn University}},
  location = {{Auburn, Alabama}},
  abstract = {Alternative solutions for navigating in challenging environments are in high demand as traditional methods are proving to be inadequate and/or easily manipulated in unfavorable situations. These methods are of particular interest in automated ground vehicle platoons due to their ability to aid and strengthen, or revamp pre-existing navigation systems entirely. Automated platoons benefit from these methods as they improve positioning capabilities required by autonomous following vehicles to accurately follow or duplicate the path of a human driven lead vehicle. There currently exist numerous platooning methods that utilize ground vehicle-based GPS/GNSS, UWB, lidar, radar and cameras. Unfortunately, the sensors used in these ground-based solutions suffer from sensor-specific deficiencies such as degraded, interfered, and unavailable signals, multipath, noise, etc. In addition, issues arise when a clear LOS cannot be established between vehicles forming the platoon. This thesis aims to address these shortcomings by leveraging visual information of the automated platoon provided by a gimballed, downward-facing monocular camera fixed to a multi-rotor UAV. Three methods utilizing the UAV’s bird’s-eye view are developed in this work; the first being a method for geo-localizing detected and tracked ground vehicles from a GPS/GNSS-aided UAV. Accurate global or relative positioning of the ground vehicles is a requirement for successfully implementing an automated ground vehicle platoon. This method explores the ability of employing an automated platoon utilizing indirect estimates of the ground vehicles’ global positions. By using measurements of the UAV’s GPS position, compass heading, and relative altitude (AGL), the aerial camera’s intrinsic and extrinsic parameters, and measurements of the pixel location of the vehicles in the aerial imagery, an estimate of the ground vehicles’ global positions within a local navigation frame can be determined. The described method can extend positioning capabilities when GPS/GNSS signals are unobtainable by the ground vehicles but available to the UAV. While no filtering, state estimation or DGPS/DRTK techniques are applied, the algorithm is shown to be most effective under small changes in the UAV’s pose with global positioning accuracy of two ground vehicles in the range of \textasciitilde 2-3m. Normal motions of the UAV is shown to increase global positioning inaccuracy north of \textasciitilde 5-7m. While this is the case, it is recognized that positioning errors across multiple tracked vehicles are affected in a similar manner. This phenomenon is comparable to multiple GPS receivers within close proximity that are affected by similar atmospheric and ephemeris errors. As a result, relative positioning between two ground vehicles is investigated in which relative positioning error is in the range of \textasciitilde 1m despite normal UAV maneuvers. The second method investigates ground vehicle relative positioning for automated platoons using aerial imagery exclusively. This method can facilitate LOS vehicle following and non-LOS path-duplication for an automated ground vehicle platoon using only the UAV’s AGL, the aerial camera’s intrinsic, and the location and heading of the tracked vehicles in the aerial imagery. The algorithm does not depend on global positioning, therefore offering a GPS/GNSS independent navigation solution for the automated platoon. Following accuracy is analyzed based on the lateral path error of an autonomous following vehicle replicating the path of a human driven lead vehicle for the two platooning methods. LOS vehicle following demonstrates lateral path accuracy less than 0.5m for close distance straight line driving maneuvers. Following accuracy decreases through circular or turning maneuvers especially with increasing separation distance between the vehicles. Non-LOS path-duplication demonstrates lateral path accuracy in the range of 5-6m for normal driving maneuvers and less than 2m for coordinated maneuvers. When utilizing relative positioning for path-duplication, accurate estimates of the following vehicle’s odometry is required for path management and dead reckoning. While ground vehicle-based sensors exist to obtain odometry information, the technology may not always be accessible. Therefore, the final method presents an aerial vision-based speed estimation algorithm for estimating the speed of the ground vehicles. Analysis is conducted on the method’s usability to support a real-time implementation of an automated ground vehicle platoon. Specifically, the analysis is focused on the algorithm’s effectiveness in reducing the reliance on the following vehicle’s INS used for path management and dead-reckoning purposes when implementing path duplication. Average velocity error across multiple post processed experimental runs is less than 1 m/s, however, computing power is a concerning limitation that can limit real-time implementation and replace the following vehicle’s INS.},
  langid = {english},
  pagetotal = {182},
  file = {C\:\\Users\\nsm0014\\Zotero\\storage\\LLBG9KWV\\Aerial Vision Based UAVUGV Cooperative Navigation for Automated Ground Vehicle Platoons 2023-07-24 19_19_42.pdf}
}

