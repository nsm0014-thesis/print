@inproceedings{guoIdentificationAttitudeEstimation2020,
  title = {Identification, {{Attitude Estimation}} and {{Capture}} of {{Tumbling Spacecraft Based}} on {{SLAM}}},
  author = {Guo, Chengjun and Deng, Shiyan and Xu, Yalan and He, Jing},
  date = {2020-10-28},
  pages = {300--309},
