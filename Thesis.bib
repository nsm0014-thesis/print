@inproceedings{suzukiNLOSMultipathDetection2020a,
  title = {{{NLOS Multipath Detection}} Using {{Convolutional Neural Network}}},
  author = {Suzuki, Taro and Kusama, Kazuki and Amano, Yoshiharu},
  date = {2020-10-28},
