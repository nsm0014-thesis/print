@inproceedings{partickeImprovementsPedestrianMovement2018,
  title = {Improvements in Pedestrian Movement Prediction by Considering Multiple Intentions in a {{Multi-Hypotheses}} Filter},
  booktitle = {2018 {{IEEE}}/{{ION Position}}, {{Location}} and {{Navigation Symposium}} ({{PLANS}})},
  author = {Particke, Florian and Hiller, Markus and Feist, Christian and Thielecke, Jorn},
