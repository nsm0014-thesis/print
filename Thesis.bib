@inproceedings{suzukiPreciseVehicleLocalization2019,
  title = {Precise {{Vehicle Localization}} Based on {{Graph Optimization}} with {{Time-Relative RTK}}–{{GNSS}}},
  author = {Suzuki, Taro},
