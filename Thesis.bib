@inproceedings{cristodaroGNSSINSVisual2018,
  title = {{{GNSS}}/{{INS}}/{{Visual Deeply Coupled Integration}}: {{Preliminary Investigation}} on {{Dynamic Jammed Datasets}}},
  shorttitle = {{{GNSS}}/{{INS}}/{{Visual Deeply Coupled Integration}}},
  author = {Cristodaro, Calogero and Ruotsalainen, Laura and Dovis, Fabio},
