@inproceedings{guoMultisensorFusionAutonomous2020,
  title = {Multi-Sensor {{Fusion}} for {{Autonomous Deep Space Navigation}}},
  author = {Guo, Chengjun and Deng, Shiyan and Xu, Yalan and He, Jing},
  date = {2020-10-28},
  pages = {290--299},
  doi = {10.33012/2020.17541},
