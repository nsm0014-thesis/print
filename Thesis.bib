@inproceedings{gntherKeplerSatelliteNavigation2018,
  title = {Kepler � {{Satellite Navigation}} without {{Clocks}} and {{Ground Infrastructure}}},
  author = {G�nther, Christoph},
  date = {2018-10-26},
  pages = {849--856},
