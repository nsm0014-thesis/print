@inproceedings{kangGNSSBasedPreciseOrbit2018,
  title = {{{GNSS-Based Precise Orbit Determination}} of {{LEO Satellites Using Un-Differenced}} and {{Double-Differenced Observations}}},
  author = {Kang, Zhigui and Tapley, Byron and Bettadpur, Sinivas},
  date = {2018-10-26},
  pages = {1290--1298},
  location = {{Miami, Florida}},
  doi = {10.33012/2018.16098},
