@inproceedings{kandemirProbabilisticMeasurementMethod2018,
  title = {A Probabilistic Measurement Method for Terrain Aided Navigation for Airborne Platforms},
  booktitle = {2018 {{IEEE}}/{{ION Position}}, {{Location}} and {{Navigation Symposium}} ({{PLANS}})},
  author = {Kandemir, Gorkem and Bingol, Haluk Erdem},
