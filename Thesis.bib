@inproceedings{sepulvedaOptimizingBankKalman2021,
  title = {Optimizing a {{Bank}} of {{Kalman Filters}} for {{Navigation Integrity}} Using {{Efficient Software Design}}},
  author = {Sepulveda, Luis E. and Leishman, Robert C. and Kauffman, Kyle and Gipson, Jonathon S.},
