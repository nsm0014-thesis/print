@inproceedings{roncaglioloMultipathAwareCarrierTracking2018,
  title = {Multipath-{{Aware Carrier Tracking Loop}} for {{High Dynamics Real-Time GNSS Receivers}}},
  author = {Roncagliolo, Pedro A. and Garc�a, Javier G.},
  date = {2018-10-26},
