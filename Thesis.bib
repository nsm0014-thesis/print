@inproceedings{siqueiraWeightedDilutionPrecision2021,
  title = {Weighted {{Dilution}} of {{Precision}} ({{WPDOP}}) with {{Consideration}} of the {{Range Errors}} and {{Elevation}} of {{GNSS Satellites During}} the {{High Ionospheric Activity Period}} for the {{Equatorial Region}}},
  author = {Siqueira, Loram and Galera, João Francisco},
  date = {2021-10-13},
