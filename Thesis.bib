@article{hernandezGNSSMachineLearning,
  title = {{{GNSS Machine Learning Toolset}} ({{MLT}})},
  author = {Hernandez, Luis and Morales, Clarizza and Aarestad, Jim and Zufelt, Brian and Yazdi, Renee and Slimak, Kevin and Nadeau, Madeleine and Choi, David and Mott, Colton},
  abstract = {For decades, the existing GPS infrastructure has proven to be the gold standard for providing an accurate Positioning, Navigation, and Timing (PNT) solution. However, the system design does not allow for rapid response to new threats. A common way of characterizing the decision-making process, called the Observe-Orient-Decide-Act (OODA) loop, can be used to describe how a PNT system can respond to threats. The system detects (Observe) a threat, analyzes it and identifies mitigation strategy options (Orient), selects an option (Decide) and implements the option (Act). The OODA loop for responding to a GPS signal threat has traditionally been a lengthy process that involves tremendous analysis and research. In addition, when a new signal, such as M-Code or CHIMERA are developed to mitigate new threats, there still exists the challenge of deploying the new signal. In short, the process of detecting a threat in the field, researching a mitigation strategy, and deploying the solution to the user, space, and control segment of the GPS infrastructure takes years to complete. However, with a re-configurable GPS constellation, a new signal or mitigation technique can be deployed quicker than ever. An on-orbit re-configurable GPS constellation provides new possibilities of accelerating the OODA loop. The Air Force Research Laboratory (AFRL) has been exploring the new capabilities a re-configurable GPS infrastructure will bring. The research project, titled Rapid-OODA, focuses on reducing the time to respond to new threats from years to days, or even hours. The Rapid-OODA effort involves research into multiple disciplines from network design, hardware development, and signals research. This paper will outline the machine learning application research and development of the Rapid-OODA project.},
  langid = {english},
  keywords = {⛔ No DOI found},
  file = {C\:\\Users\\nsm0014\\Zotero\\storage\\TY98WGX6\\Hernandez et al. - GNSS Machine Learning Toolset (MLT).pdf}
}

@inproceedings{hernandezPrivacypreservingCooperativePositioning2020,
