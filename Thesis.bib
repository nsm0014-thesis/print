@inproceedings{tuckerGPSGNSSInterference2020,
  title = {{{GPS}}/{{GNSS Interference Power Difference}} of {{Arrival}} ({{PDOA}}) {{Localization Weighted}} via {{Nearest Neighbors}}},
  author = {Tucker, Johnathan A. and Puskar, Charles and Lee, Chiawei and Akos, Dennis},
  date = {2020-10-28},
  pages = {3550--3560},
