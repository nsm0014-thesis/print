@inproceedings{renaudinTOAErrorBounds2021,
  title = {{{TOA Error Bounds}} for {{Positioning}} in {{5G New Radio Networks}}},
  author = {Renaudin, Olivier and Salcedo, José A. López and Seco-Granados, Gonzalo and Lapin, Ivan and Zanier, Francesca and Ries, Lionel},
  date = {2021-10-13},
  pages = {2940--2956},
  location = {{St. Louis, Missouri}},
  doi = {10.33012/2021.18000},
  url = {https://www.ion.org/publications/abstract.cfm?articleID=18000},
