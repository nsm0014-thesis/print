@inproceedings{el-mowafySecondGenerationSBAS2019,
  title = {Second {{Generation SBAS}} – {{Performance Analysis}} and {{Bridging Positioning}} and {{Integrity Monitoring}} during {{SBAS Outages}} in the {{Urban Environment}}},
