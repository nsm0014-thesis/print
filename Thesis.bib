@article{harimaTransmissionAugmentationMessages,
  title = {Transmission of {{Augmentation Messages}} for {{Precise Point Positioning Utilizing Japanese QZSS LEX Signal}}},
  author = {Harima, Ken and Choy, Suelynn and Wakabayashi, Yaka and Kogure, Satoshi and Rizos, Chris},
  abstract = {The Quasi-Zenith Satellite System (QZSS) is a Japanese regional navigation satellite system covering East Asia and Oceania region. QZSS is designed as a satellite-based augmentation system to improve the availability and performance of Global Navigation Satellite Systems (GNSS) in the region. Besides L1, L2 and L5 navigation signals, QZSS also transmits augmentation signals such as the LEX (L-band Experimental) signal, which is capable of transmitting precise navigation messages for high accuracy point positioning. A joint project between the Australian Cooperative Research Centre for Spatial Information (CRCSI) and the Japanese Aerospace Exploration Agency (JAXA) aims to assess the feasibility of using the LEX signal to potentially deliver a precise positioning service in Australia. This paper describes a preliminary test for the transmission of a prototype “Australian-generated LEX corrections”. The research was performed in November 2013 and June 2014. Correction messages, based on the IGS and CNES realtime products were transmitted from Australia to the QZS master control station in Japan and broadcasted on the LEX signal. The messages were received and decoded in Australia and used to calculate precise point positioning (PPP) solutions. The time difference between the message’s time of frame (TOF) and the time of arrival to the rover receiver were compared between messages transmitted on the LEX signal and through a land network. The real time PPP solutions obtained from the developed “Australian LEX” system have comparable performance to those obtained using the IGS real-time products obtained through a land network, showing that latency and reliability of the system is adequate for real-time PPP. In the future regional orbits and clocks for real-time PPP will be generated and delivered through the QZSS LEX signal.},
  langid = {english},
  keywords = {⛔ No DOI found},
  file = {C\:\\Users\\nsm0014\\Zotero\\storage\\H2CDVVEE\\Harima et al. - Transmission of Augmentation Messages for Precise .pdf}
}

