@inproceedings{challonerNewIMUDigitally2018,
  title = {A New {{IMU}} with a Digitally Controlled {{PZT CVG}}},
  booktitle = {2018 {{IEEE}}/{{ION Position}}, {{Location}} and {{Navigation Symposium}} ({{PLANS}})},
  author = {Challoner, Anthony Dorian and Daniel, Jeremy and Bond, Peter and Beitia, Jose and Li, Rongsheng Ken},
