@inproceedings{huangCoarsetoFineLiDARBasedSLAM2021,
  title = {A {{Coarse-to-Fine LiDAR-Based SLAM}} with {{Dynamic Object Removal}} in {{Dense Urban Areas}}},
  author = {Huang, Feng and Shen, Donghui and Wen, Weisong and Zhang, Jiachen and Hsu, Li-Ta},
  date = {2021-10-13},
