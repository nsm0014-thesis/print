@inproceedings{kanhereFactorGraphbasedSpoofing2022,
  title = {Factor {{Graph-based Spoofing Mitigation}} Using the {{Chimera Signal Enhancement}}},
  author = {Kanhere, Ashwin Vivek and Mina, Tara and Shetty, Akshay and Gao, Grace},
