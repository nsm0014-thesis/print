@inproceedings{reitbauerPerformanceAnalysisGNSS2021,
  title = {Performance {{Analysis}} of {{GNSS}}/{{INS}}/{{VO}}/{{Odometry Sensor Fusion Algorithms}} for {{Tracked Agricultural Vehicles}}},
  author = {Reitbauer, Eva and Schmied, Christoph},
  date = {2021-10-13},
  pages = {3250--3262},
